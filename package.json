{
  "name": "XuanMaiApi",
  "version": "1.0.0",
  "description": "A Node.js app using Express, Sequelize, TypeScript, and PostgreSQL",
  "author": "Nhat",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf build node_modules yarn-error.log yarn.lock",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "dev": "set NODE_ENV=development && nodemon",
    "prod": "npm run build && set NODE_ENV=production && node build/index.js",
    "build": "swc src -d build --source-maps --copy-files",
    "build:tsc": "tsc && tsc-alias",
    "start": "node build/index.js",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --ignore-path .gitignore --ext .ts src/ --fix",
    "migrate": "sequelize-cli db:migrate",
    "migrate:make": "sequelize-cli migration:generate --name",
    "migrate:status": "sequelize-cli db:migrate:status",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:undo:file": "sequelize-cli db:migrate:undo --to",
    "seed": "sequelize-cli db:seed:all",
    "seed:dev": "set NODE_ENV=development && sequelize-cli db:seed:all",
    "seed:make": "sequelize-cli seed:create --name",
    "rollback": "sequelize-cli db:seed:undo",
    "rollback:all": "sequelize-cli db:seed:undo:all",
    "rollback:file": "sequelize-cli db:migrate:undo --name",
    "db:init": "npm run start && npm run migrate && npm run seed",
    "db:drop": "sequelize-cli db:drop",
    "db:create": "sequelize-cli db:create",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev"
  },
  "keywords": [
    "node",
    "express",
    "typescript",
    "sequelize",
    "postgresql"
  ],
  "dependencies": {
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.15.0",
    "fs-extra": "^11.1.0",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "pusher": "^5.1.2",
    "redis": "^4.6.5",
    "sequelize": "^6.28.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.36",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.1",
    "@types/nodemailer": "^6.4.7",
    "@types/pg": "^8.6.6",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.2.0",
    "node-config": "^0.0.2",
    "node-gyp": "^9.3.1",
    "nodemon": "^2.0.20",
    "pm2": "^5.2.2",
    "prettier": "^2.8.4",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
